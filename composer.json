{
  "name": "toplan/phpsms",
  "description": "sms send library for php, support the main SMS service provider.",
  "license": "MIT",
  "version": "1.2.0",
  "keywords": ["sms", "php sms", "phpsms", "sms library for php"],
  "authors": [
    {
      "name": "toplan",
      "email": "toplan710@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "toplan/task-balancer": "~0.2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Toplan\\PhpSms\\": [
        "src/phpsms/",
        "src/phpsms/agents/",
        "src/phpsms/facades/"
      ]
    },
    "classmap": [
      "src/phpsms/lib/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability" : "dev"
}
